{
  "restructuredtext.confPath": "${workspaceFolder}/docs",
  "grammarly.selectors": [
    {
      "language": "restructuredtext",
      "scheme": "file"
    }
  ],
  "grammarly.config.suggestions.OxfordComma": true,
  "grammarly.config.suggestions.ConjunctionAtStartOfSentence": true,
  "grammarly.config.suggestions.PrepositionAtTheEndOfSentence": true,
  "grammarly.config.suggestions.PassiveVoice": false,
  "grammarly.config.suggestions.UnnecessaryEllipses": false,
  "grammarly.config.documentDomain": "general",

}
